<div class="col-sm-11 col-xs-12 blog-content">
  <div id="title">
  <h2 class="text-center"><%= @post.title %></h2>
  <h5 class="text-center"><%= @post.created_at.strftime('%b %d, %Y') %></h5></div>
  <div id="show-content"><%= raw @post.body %></div>
  <td><%= link_to 'Back', posts_path %></td>
  <td><%= link_to 'Edit Post', edit_post_path %></td>
</div>
